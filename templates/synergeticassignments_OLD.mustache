<table class="table table-striped table-responsive text-center">
    <thead>
       <tr class="attheader">
          <th>{{#str}} learningarea, block_assignmentsquizzes_report {{/str}}</th>
          <th>{{#str}} assessheading, block_assignmentsquizzes_report {{/str}}</th>
          <th>Assessment number</th>
          <th>{{#str}} assessmentdescription, block_assignmentsquizzes_report {{/str}}</th>
          <th>{{#str}} markoutof, block_assignmentsquizzes_report {{/str}}</th>
          <th>{{#str}} rank, block_assignmentsquizzes_report {{/str}}/{{#str}} noincohort, block_assignmentsquizzes_report {{/str}}</th>
          <th>{{#str}} cohortmeanscore, block_assignmentsquizzes_report {{/str}}</th>
          <th>{{#str}} weighting, block_assignmentsquizzes_report {{/str}}</th>
          <th>{{#str}} testdate, block_assignmentsquizzes_report {{/str}}</th>
          
       </tr>
    </thead>
    <tbody>
        {{#details}}
             {{#subjects}}
                <tr>
                    <td  class="areaheading">{{area}}</td>
                </tr>
                    {{#subject}}
                    <tr>
                        <td></td>
                        <td>{{subject}}</td>
                        <td>{{assesnumber}}</td>
                        <td>{{assessareaoverview}}</td>
                        <td><b>{{raw}}</b> ({{outof}})</td>
                        <td>{{rank}}/{{numberinclass}}</td>
                        <td>{{meanscore}}</td>
                        <td>{{weighting}}</td>
                        <td>{{testdate}}</td>
                    </tr>
                    {{/subject}}
                {{/subjects}}
        {{/details}}

    </tbody>
 </table>